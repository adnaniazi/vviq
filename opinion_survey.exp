**********************************************************************************************************************************************************************************
<radiobuttons instructions>
/ caption = "Were the instructions clear to you"
/ options = ("Yes", "No")
</radiobuttons>
**********************************************************************************************************************************************************************************
<slider interesting>
/ caption = "How interesting or boring was this experiment for you? (move the slider)"
/ labels = ("very~ninteresting", "moderatly~ninteresting ", "slightly~ninteresting ", "no~nopinion", "slightly~nboring ", "moderalty~nboring ", "very~nboring")
/ range = (1, 7)
/ slidersize = (60%, 5%)
/ showtooltips = false
</slider>
**********************************************************************************************************************************************************************************
<slider vividness>
/ caption = "How vividly and clearly were you able imagine the various alpahbets during the experiment? (move the slider)"
/ labels = ("Perfectly clear and perfectly vivid~nas if I was seeing the alphabets ", "Clear and~nreasonably vivid", "Moderately clear~nand vivid", "Vague~nand dim", "No image~nat all")
/ range = (1, 5)
/ slidersize = (60%, 5%)
/ showtooltips = false
</slider>
**********************************************************************************************************************************************************************************

<radiobuttons eyesight>
/ caption = "Do you normally use glasses or contact lenses?"
/ options = ("Yes", "No")
</radiobuttons>
**********************************************************************************************************************************************************************************
<textbox deviation>
/ caption = "If your answer to the previous question was 'Yes', then how much is the deviation in your left and right eye ? If you don't remember it exactly just wirte a rough estimate. "
/ required = false
/ multiline = true
/ textboxsize=(85,5)
</textbox>
**********************************************************************************************************************************************************************************
<radiobuttons headache>
/ caption = "Are you feeling any headache after the experiment?"
/ options = ("Yes", "No")
</radiobuttons>
**********************************************************************************************************************************************************************************
<radiobuttons contactlenses>
/ caption = "Have you used contact lenses during the experiment?"
/ options = ("Yes", "No")
</radiobuttons>
**********************************************************************************************************************************************************************************
<radiobuttons glasses>
/ caption = "Have you used glasses during the experiment?"
/ options = ("Yes", "No")
</radiobuttons>
**********************************************************************************************************************************************************************************

<radiobuttons video>
/ caption = "Would you like to receive an MRI video of your brain and your face in 3D, as a souvenir for your participation in the experiment?"
/ options = ("Yes", "No")
</radiobuttons>
**********************************************************************************************************************************************************************************
<textbox videoemail>
/ caption = "If your answer to the previous question was 'Yes', then please write down your GMAIL or YAHOO or MSN email address. We will email you the video on that address."
/ required = false
/ multiline = true
/ textboxsize=(85,5)
</textbox>
**********************************************************************************************************************************************************************************
<textbox comments>
/ caption = "If you have any comments or questions, please write them down."
/ required = false
/ multiline = true
/ textboxsize=(85,5)
</textbox>
**********************************************************************************************************************************************************************************
<checkboxes alphabets>
/ caption = "Which of the following alphabets did you find difficult to imagine? (you can check multiple boxes, if you want to)."
/ options = ("D","O", "N", "E", "R","S","I", "T","U")
</checkboxes>
