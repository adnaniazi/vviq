<surveypage demographics1>
/ caption = "Demographic Questionionnaire (Page 1 of 3)"
/ fontstyle = ("Verdana", -16, true, false, false, false, 5, 0)
/ questions = [1=age; 2=sex;  3=nationality; 4=interesting; 5=eyesight; 6=deviation] 
</surveypage>


<surveypage demographics2>
